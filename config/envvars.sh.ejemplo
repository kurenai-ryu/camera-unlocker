#!/bin/sh
#sample enviroment, usar con source
#ejemplo
#  source configuracion/envvars.sh
# debe ejecutarlo el usuario que accederá a config

#base de datos principal
#del servidor con los datos de usuario
export PGHOST="localhost"
export PGPORT="5432"
